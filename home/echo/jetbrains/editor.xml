<application>
    <component name="CodeInsightSettings">
        <option name="HIGHLIGHT_SCOPE" value="true" />
        <option name="USE_INSTANCEOF_ON_EQUALS_PARAMETER" value="true" />
        <option name="ADD_UNAMBIGIOUS_IMPORTS_ON_THE_FLY" value="true" />
        <EXCLUDED_PACKAGE NAME="com.sun.*" />
        <EXCLUDED_PACKAGE NAME="sun.*" />
        <EXCLUDED_PACKAGE NAME="java.awt.Component" />
        <EXCLUDED_PACKAGE NAME="java.awt.List" />
        <EXCLUDED_PACKAGE NAME="java.rmi.registry.Registry" />
        <EXCLUDED_PACKAGE NAME="java.sql.Date" />
        <EXCLUDED_PACKAGE NAME="javax.swing.text.html.*" />
        <EXCLUDED_PACKAGE NAME="org.w3c.dom.Entity" />
    </component>
    <component name="DaemonCodeAnalyzerSettings">
        <option name="SHOW_METHOD_SEPARATORS" value="true" />
    </component>
    <component name="EditorSettings">
        <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
        <option name="SOFT_WRAP_FILE_MASKS" value="*" />
        <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
        <option name="REMOVE_TRAILING_BLANK_LINES" value="true" />
    </component>
    <component name="InlineCompletionOnboarding">
        <option name="onboardingFinished" value="true" />
    </component>
    <component name="JavaCodeFoldingSettings">
        <option name="collapseAccessors" value="true" />
    </component>
    <component name="TodoConfiguration">
        <pattern pattern="\btodo\b.*" />
        <pattern pattern="\bfixme\b.*" />
        <pattern pattern="\btemp\b.*" />
        <pattern pattern="\bstub\b.*" />
    </component>
</application>